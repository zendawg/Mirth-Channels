  <preprocessingScript>var filename = muleContext.getMessage().getProperty(&quot;originalFilename&quot;);

var f = new java.io.File(new java.io.File(globalMap.get(&apos;OE_VFA_IMAGES_IN&apos;)), filename);
var size = f.length();

var fileId = addFile(f);

fileAudit(&apos;i&apos;, &apos;f&apos;, globalMap.get(&apos;OE_VFA_IMAGES_IN&apos;), filename, null, null, f.length(), -1);

var xml = &quot;&lt;vfaImage&gt;&quot;
